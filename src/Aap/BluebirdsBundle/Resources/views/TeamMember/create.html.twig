{% import 'AapBluebirdsBundle::macros/buttons.html.twig' as buttons %}

<h2>Create a team member</h2>
<form action="{{ path('team_member_create', {'team_id': team.id}) }}" method="post" {{ form_enctype(form) }}>
    <fieldset>
        {{ form_widget(form) }}
    </fieldset>
    <fieldset class="buttons">
        {{ buttons.save('Save') }}
        {{ buttons.cancel(path('club_list'), 'Cancel') }}
    </fieldset>
</form>